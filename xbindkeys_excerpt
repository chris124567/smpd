# skip to next song
"smpd -n 1"
  Mod4 + bracketright
# go to previous song
"smpd -n -1"
  Mod4 + bracketleft
# skip to next song
"smpd -n 1"
  XF86AudioNext
# go to previous song
"smpd -n -1"
  XF86AudioPrev
# toggle playing
"smpd -t"
  XF86AudioPlay
# go ahead 6 seconds in current song
"smpd -P 6"
  Mod4 + equal
# go back 6 seconds in current song
"smpd -P -6"
  Mod4 + minus
# toggle playing
"smpd -t" 
  Mod4 + p
# shuffle playlist
"smpd -R"
  Mod4 + comma
# clear playlist
"smpd -c"
  Mod4 + i

